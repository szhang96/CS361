    "def fib(n):\n",
    "    if (n == 0 or n ==1):\n",
    "        return 1\n",
    "    else:\n",
    "        return fib(n-1) + fib(n-2)\n",
    "\n",
    "for i in range(0,20):\n",
    "    print(\"fib\", i, \"is\", fib(i))"
   ]
